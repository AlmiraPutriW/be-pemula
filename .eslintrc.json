{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
    },
    "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "no-trailing-spaces": "error",
    "eol-last": ["error", "always"],
    "no-unused-vars": ["warn", { "args": "none" }]
  }
}
